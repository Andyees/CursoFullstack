<%-  include("header")  -%>


<div class="col-md-6 container text-center">

   
      
    <table class="table table-bordered">
        
      <thead>
        <tr>
        <th>ID</th>
        <th>NOMBRE  </th>
        <th>APELLIDO</th>
        <th>EDAD    </th>
        <th>CARGO   </th>
        <th>Methods</th>

        </tr>
      </thead>

      <Tbody>
        <% for (const user of Usuarios) { %>

        <tr>
           
         <td> <%= user.id %> </td>
         <td> <%= user.Name %> </td>
         <td> <%= user.Apellido %> </td>
         <td> <%= user.Age %> </td>
         <td> <%= user.Cargo %> </td>
         <td> <a href="main/ViewEdit/<%= user.id %>" class="btn btn-primary">Editar</a></td>
        </tr>
        

            
        <%    } %>


      </Tbody>
      </table>

      <a class="btn btn-success" href="/main/addUser">Agregar Usuario</a>
      <a class="btn btn-danger" href="/main/DeleteUser">Eliminar Usuario</a>

      
    </div>

  
    <%-  include("footer")  -%>